{% extends 'base.html.twig' %}
{% block title %}Modification du grade {{grade.nomGrade}} {% endblock %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}

{% block body %}

<div class="container-fluid">
    <h1>Modification du grade {{grade.nomGrade}}</h1>

    {{form_start(form)}}
{{form_widget(form)}}
<button type="submit" class="btn btn-primary">
    <i class="fa fa-check"></i>
    Enregistrer les modifications du grade
</button>
{{form_end(form)}}
</div>

{% endblock %}